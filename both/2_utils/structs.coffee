################################################################################
# Array
################################################################################

################################################################################
Array::unique = ->
  output = {}
  output[@[key]] = @[key] for key in [0...@length]
  value for key, value of output

################################################################################
# Dictionary
################################################################################

################################################################################
@merge = (dest, objs...) ->
	for obj in objs
		dest[k] = v for k, v of obj
	return dest

################################################################################
# Sets
################################################################################

################################################################################
@set_union = (a, b)->
	res = []
	for x in a
		if b.has(x)
			res.push(x)
	return res


